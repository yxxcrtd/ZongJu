<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="cn.digitalpublishing.po.RecordCompress" table="T_RECORDCOMPRESS">
        <id name="id" type="java.lang.String">
            <column name="id" length="32">
            	<comment>id</comment>
            </column>
            <generator class="uuid.hex" />
        </id>
        
        <property name="name" type="java.lang.String">
            <column name="name" length="32">
            	<comment>产品包系统名</comment>
            </column>
        </property>	
        
        <property name="originName" type="java.lang.String">
            <column name="originName" length="128">
            	<comment>文件原名</comment>
            </column>
        </property>	
        
        <property name="systemName" type="java.lang.String">
            <column name="systemName" length="32">
            	<comment>文件系统名</comment>
            </column>
        </property>	
        
        <property name="createTime" type="java.util.Date">
        	<column name="createTime" length="32">
        		<comment>生成日期</comment>
        	</column>
        </property>
        
    </class>
</hibernate-mapping>
